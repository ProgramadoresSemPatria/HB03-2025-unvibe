{
  "name": "arruma-ruyter-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "bot",
    "web-app",
    "packages/db"
  ],
  "scripts": {
    "run:bot": "npm run build --workspace=arruma-ruyter && npm start --workspace=arruma-ruyter",
    "build:bot": "npm run build --workspace=arruma-ruyter",
    "dev:web": "npm run dev --workspace=web-app",
    "build:web": "npm run build --workspace=web-app",
    "start:web": "npm start --workspace=web-app"
  }
}

